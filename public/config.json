{
    "workflowApiVersion": "1.1",
    "name": "Custom Activity Name",
    "description": "Custom Activity Description",
    "metaData": {
      "icon": "images/icon.svg",
      "category": "Custom"
    },
    "type": "REST",
    "arguments": {
      "execute": {
        "inArguments": [
          {
            "contactKey" : "{{Contact.Key}}",
            "FirstName" : "{{Contact.Attribute.JourneyEntrySource.FirstName}}",
            "LastName" : "{{Contact.Attribute.JourneyEntrySource.LastName}}"
          }
        ],
        "url": "https://customsfmc-production.up.railway.app/execute"
      }
    },
    "configurationArguments": {
      "publish": {
        "url": "https://customsfmc-production.up.railway.app/publish"
      }
    },
    "copySettings": {
      "allowCopy": true,
      "ignoreArguments": ["eventDefinitionKey"]
    }
  }