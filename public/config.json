{
  "workflowApiVersion": "1.1",
  "name": "Custom Activity Name",
  "description": "Custom Activity Description",
  "metaData": {
    "icon": "images/icon.svg",
    "category": "Custom",
    "isConfigured": true
  },
  "type": "RESTDECISION",
  "arguments": {
    "execute": {
      "inArguments": [
        {
          "ContactKey": "{{Contact.Key}}",
          "FirstName": "{{Event.eventDefinitionKey.FirstName}}",
          "LastName": "{{Event.eventDefinitionKey.LastName}}",
          "IdOT": "{{Event.eventDefinitionKey.OrdenTrabajo}}"
        }
      ],
      "outArguments": [
        {
          "confirmacion": 0
        }
      ],
      "timeout": 10000,
      "url": "https://customsfmc-production.up.railway.app/execute"
    }
  },
  "configurationArguments": {
    "applicationExtensionKey": "61defa1a-0a63-4f8f-b81c-5d4f5aa1d13e",
    "publish": {
      "url": "https://customsfmc-production.up.railway.app/publish"
    },
    "save": {
      "url": "https://customsfmc-production.up.railway.app/save"
    },
    "validate": {
      "url": "https://customsfmc-production.up.railway.app/validate"
    }
  },
  "outcomes": [
    {
      "arguments": {
        "branchResult": "buy_item",
        "some argument": "passed from config.json for buy_item"
      }
    },
    {
      "arguments": {
        "branchResult": "sell_item",
        "some argument": "passed from config.json for sell_item"
      }
    },
    {
      "arguments": {
        "branchResult": "hold_item",
        "some argument": "passed from config.json for hold_item"
      },
      "metaData": {
        "label": "MyCustomLabel"
      }
    }
  ],
  "schema": {
    "arguments": {
      "execute": {
        "inArguments": [
          {
            "FirstName": {
              "dataType": "Text",
              "isNullable": false,
              "access": "Visible",
              "direction": "In"
            },
            "ContactKey": {
              "dataType": "Text",
              "isNullable": false,
              "access": "Visible",
              "direction": "In"
            },
            "LastName": {
              "dataType": "Text",
              "isNullable": false,
              "access": "Visible",
              "direction": "In"
            },
            "IdOT": {
              "dataType": "Number",
              "isNullable": false,
              "access": "Visible",
              "direction": "In"
            }
          }
        ],
        "outArguments": [
          {
            "confirmacion": {
              "dataType": "Number",
              "isNullable": false,
              "access": "Visible",
              "direction": "Out"
            }
          }
        ]
      }
    }
  }
}